{
  "name": "ITAP3012",
  "dockerComposeFile": ["../docker-compose.yml"],
  "service": "web",
  "runServices": ["web", "db", "phpmyadmin"],
  "overrideCommand": false,
  "workspaceFolder": "/workspaces/ITAP3012",
  "forwardPorts": [8080, 8081, 3306],
  "portsAttributes": {
    "8080": { "label": "Apache (PHP)" },
    "8081": { "label": "phpMyAdmin" },
    "3306": { "label": "MariaDB" }
  },
  "postCreateCommand": "bash /workspaces/ITAP3012/docker/web/post-create.sh"
}
